# Generated by stackctl init. Fill secrets before running apply.
STACK_ENV={{.Env}}
DOMAIN={{.Domain}}
ADMIN_EMAIL={{.Email}}
TZ=UTC

# Core app settings
FRONTEND_IMAGE=nginx:1.27-alpine
BACKEND_IMAGE=ghcr.io/example/backend:latest
KEYCLOAK_IMAGE=quay.io/keycloak/keycloak:25.0

# Databases
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_me_postgres
POSTGRES_DB=app
MYSQL_ROOT_PASSWORD=change_me_mysql
MYSQL_DATABASE=app
MYSQL_USER=app
MYSQL_PASSWORD=change_me_mysql_user

# Grafana and observability defaults
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=change_me_grafana

# Optional restic backup target
# RESTIC_REPOSITORY=s3:https://s3.example.com/stackctl
# RESTIC_PASSWORD=change_me_restic
# AWS_ACCESS_KEY_ID=replace_me
# AWS_SECRET_ACCESS_KEY=replace_me
